    <keyboard-shortcut first-keystroke="ctrl alt e" />
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="MethodUp">
    <keyboard-shortcut first-keystroke="ctrl alt a" />
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
