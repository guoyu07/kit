    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="shift s" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="Vcs.ShowHistoryForBlock">
    <keyboard-shortcut first-keystroke="shift ctrl g" second-keystroke="shift h" />
  </action>
  <action id="Vcs.ShowTabbedFileHistory">
    <keyboard-shortcut first-keystroke="shift ctrl g" second-keystroke="h" />
  </action>
  <action id="ViewBreakpoints">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="b" />
  </action>
</keymap>