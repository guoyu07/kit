<keymap version="1" name="Idea-Emacs-Kit" parent="Emacs">
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="shift insert" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorJoinLines">
    <keyboard-shortcut first-keystroke="shift alt 6" />
  </action>
  <action id="EditorToggleCase">
    <keyboard-shortcut first-keystroke="alt u" />
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="shift ctrl n" second-keystroke="i" />
  </action>
  <action id="GotoSuperMethod">
    <keyboard-shortcut first-keystroke="shift ctrl n" second-keystroke="u" />
  </action>
  <action id="GotoTypeDeclaration">
    <mouse-shortcut keystroke="shift control button1" />
    <keyboard-shortcut first-keystroke="shift ctrl n" second-keystroke="t" />
  </action>
  <action id="MethodDown">
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="MethodUp">
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="ReloadFromDisk">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="u" />
  </action>
  <action id="ShowProjectStructureSettings">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="s" />
  </action>
